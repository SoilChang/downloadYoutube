{
    "description": "Download youtube video dynamically",
    "manifest_version": 2,
    "name": "DY",
    "version": "1.0",
    "permissions": [
        "*://www.youtube.com/*",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "browser_action.html",
      "default_icon": "onlineVideoConverter.png"
    }
}